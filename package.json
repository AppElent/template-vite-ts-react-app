{
  "name": "template-vite-ts-react-app",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:fix": "npm run lint-fix && tsc -b && vite build",
    "tsc": "tsc -b",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint-fix": "eslint --fix ./src",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,mdx}\" --cache",
    "deploy:stg": "npm run lint-fix && npm run build && firebase deploy --only hosting:stg",
    "deploy:stg:skipchecks": "npm run build && firebase deploy --only hosting:stg",
    "deploy:stg:skipbuild": "firebase deploy --only hosting:stg",
    "deploy:prd": "npm run lint-fix && npm run build && firebase deploy --only hosting:prd",
    "deploy:prd:skipchecks": "npm run build && firebase deploy --only hosting:prd",
    "deploy:prd:skipbuild": "firebase deploy --only hosting:prd",
    "deploy": "npm run deploy:stg && npm run deploy:prd:skipbuild",
    "deploy:version": "npm run update-version && npm run deploy",
    "update-version": "node update-version.js",
    "check-updates": "npx npm-check-updates -i",
    "docs": "jsdoc -c jsdoc.conf.json",
    "npm-check": "npm-check"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.0",
    "@faker-js/faker": "^9.4.0",
    "@hello-pangea/dnd": "18.0.0-beta.0",
    "@microlink/react-json-view": "^1.24.0",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.4.1",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "firebase": "^11.2.0",
    "formik": "^2.4.6",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.2",
    "lodash": "^4.17.21",
    "material-ui-confirm": "^3.0.18",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.2.0",
    "react-i18next": "^15.4.0",
    "react-quill-new": "^3.3.3",
    "react-router-dom": "^7.1.3",
    "react-toastify": "^11.0.3",
    "translation-check": "^1.1.0",
    "use-query-params": "^2.2.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-react-compiler": "^19.0.0-beta-27714ef-20250124",
    "eslint": "^9.19.0",
    "eslint-plugin-react-compiler": "^19.0.0-beta-27714ef-20250124",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prompt-sync-plus": "^1.0.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11"
  }
}
